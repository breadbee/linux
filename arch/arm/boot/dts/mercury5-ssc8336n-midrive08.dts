	 wifi_pwr: fixedregulator@0 {
		compatible = "regulator-fixed";
		regulator-name = "wifi_pwr";
		regulator-min-microvolt = <3300000>;
		regulator-max-microvolt = <3300000>;
		gpio = <&gpio_pm 5 0>;
	};

};

&cpu0 {
	cpu-supply = <&vcc_core>;
};

&pm_uart {
	status = "okay";
};

&isp {
	status = "okay";
	nor: nor@0 {
		#address-cells = <1>;
		#size-cells = <1>;
		compatible = "jedec,spi-nor";
		reg = <0>;
		spi-max-frequency = <40000000>;
	};
};

&i2c0 {
	status = "okay";
	ip6303: ip6303@30 {
		compatible = "injoinic,ip6303-regulator";
		reg = <0x30>;
		regulators {
			vcc_core: dc1 {
				regulator-name = "vcc-core";
				regulator-min-microvolt = <600000>;
				regulator-max-microvolt = <1200000>;
				regulator-always-on;
			};

			vcc_ddr: dc2 {
				regulator-name = "vcc-ddr";
				regulator-min-microvolt = <1800000>;
				regulator-max-microvolt = <1800000>;
				regulator-always-on;
			};

			vcc_io: dc3 {
				regulator-name = "vcc-io";
				regulator-min-microvolt = <3300000>;
				regulator-max-microvolt = <3300000>;
				regulator-always-on;
			};
		};
	};
};

&i2c1 {
	status = "okay";
};

&sar {
	status = "okay";
};

&sd {
	vmmc-supply = <&vcc_io>;
	vqmmc-supply = <&vcc_io>;
	cd-gpios = <&gpio_pm 7 GPIO_ACTIVE_LOW>;
	status = "okay";
};

&miu {
	ddr-supply = <&vcc_ddr>;
};

&usbphy0 {
	mstar,utmi-dxswap;
	status = "okay";
};

&uhc0 {
	status = "okay";
};

&usbphy1 {
	mstar,utmi-dxswap;
	status = "okay";
};

&uhc1 {
	status = "okay";
};
